    "null-loader": "^0.1.1",
    "protractor": "^5.2.2",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "typescript": "2.4.2"
  },
  "description": "Cyface's Cardgame",
  "cordova": {
