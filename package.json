  "devDependencies": {
    "@ionic/app-scripts": "^3.1.2",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.8",
    "angular2-template-loader": "^0.6.2",
    "cordova": "^7.1.0",
    "html-loader": "^0.4.5",
