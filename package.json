    "@ionic-native/core": "^4.4.2",
    "@ionic-native/keyboard": "^4.4.2",
    "@ionic-native/splash-screen": "^4.3.3",
    "@ionic-native/status-bar": "^4.3.3",
    "@ionic/storage": "2.1.3",
    "cordova-android": "^6.3.0",
    "cordova-browser": "^5.0.1",
